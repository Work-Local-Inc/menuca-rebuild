{
  "name": "menuca-rebuild",
  "version": "1.0.0",
  "description": "MenuCA Multi-tenant SaaS Platform",
  "main": "dist/server/app.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon src/server/app.ts",
    "dev:server:mock": "ts-node --project tsconfig-scripts.json src/server/mock-app.ts",
    "dev:client": "next dev -p 3001",
    "dev:mock": "concurrently \"npm run dev:server:mock\" \"npm run dev:client\"",
    "build": "next build",
    "build:server": "tsc",
    "build:client": "next build",
    "start": "npm run start:server",
    "start:server": "node dist/server/app.js",
    "start:client": "next start -p 3001",
    "db:setup": "node dist/scripts/setup-database.js",
    "db:migrate": "node dist/scripts/migrate.js",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.51.0",
    "@types/stripe": "^8.0.416",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "compression": "^1.7.4",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.263.1",
    "morgan": "^1.10.0",
    "next": "^13.4.7",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.7.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stripe": "^18.3.0",
    "tailwind-merge": "^1.14.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.5",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.7",
    "jest": "^29.6.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.26",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "saas",
    "multi-tenant",
    "restaurant",
    "management",
    "postgresql",
    "typescript"
  ],
  "author": "MenuCA Team",
  "license": "MIT"
}
